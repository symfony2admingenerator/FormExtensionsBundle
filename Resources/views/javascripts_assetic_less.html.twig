{% block admingenerator_form_javascripts %}
    {% if s2a_formextensions_include_jquery %}
        <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/jquery/js/jquery.min.js') }}"></script>
    {% endif %}

    {% if s2a_formextensions_include_jqueryui %}
        <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/jquery-ui/js/jquery-ui.min.js') }}"></script>
    {% endif %}

    {% if s2a_formextensions_include_momentjs %}
        <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/moment/moment.min.js') }}"></script>

        {% if app.request.locale in ['af', 'ar-ma', 'ar-sa', 'ar-tn', 'ar', 'az', 'be', 'bg', 'bn', 'bo', 'br', 'bs', 'ca', 'cs', 'cv', 'cy', 'da', 'de-at', 'de', 'el', 'en-au', 'en-ca', 'en-gb', 'eo', 'es', 'et', 'eu', 'fa', 'fi', 'fo', 'fr-ca', 'fr', 'fy', 'gl', 'he', 'hi', 'hr', 'hu', 'hy-am', 'id', 'is', 'it', 'ja', 'ka', 'km', 'ko', 'lb', 'lt', 'lv', 'mk', 'ml', 'mr', 'ms-my', 'my', 'nb', 'ne', 'nl', 'nn', 'pl', 'pt-br', 'pt', 'ro', 'ru', 'sk', 'sl', 'sq', 'sr-cyrl', 'sr', 'sv', 'ta', 'th', 'tl-ph', 'tr', 'tzm-latn', 'tzm', 'uk', 'uz', 'vi', 'zh-cn', 'zh-tw'] %}
            <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/moment/locale/' ~ app.request.locale ~ '.js') }}"></script>
        {% endif %}
    {% endif %}

    {% if s2a_formextensions_include_gmaps %}
        <script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=true&libraries=places"></script>
    {% endif %}

    {% if s2a_formextensions_include_blueimp %}
    <!-- blueimp.jquery-file-upload.min.js  -->
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-javascript-templates/js/tmpl.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-load-image/js/load-image.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-canvas-to-blob/js/canvas-to-blob.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-file-upload/js/jquery.iframe-transport.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-file-upload/js/jquery.fileupload.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-file-upload/js/jquery.fileupload-process.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-file-upload/js/jquery.fileupload-image.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-file-upload/js/jquery.fileupload-validate.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/vendor/blueimp-file-upload/js/jquery.fileupload-ui.min.js') }}"></script>
    {% endif %}

    <!-- form-extensions.min.js -->
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-collection/js/bootstrap-collection.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/datetime-picker/js/bootstrap-datetimepicker.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/daterange-picker/js/daterangepicker.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-select/js/bootstrap-select.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/upload-collection/js/upload-collection.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/double-list/js/double-list.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/elastic/js/jquery.elastic.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/google-map/js/google-map.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/knob/js/jquery.knob.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/minicolors/js/jquery.minicolors.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/select2/js/select2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/single-upload/js/single-upload.min.js') }}"></script>

    <!-- locale -->
    {% if app.request.locale in ['ca', 'cs', 'da', 'de', 'es', 'et', 'eu', 'fr', 'gl', 'he', 'hr', 'hu', 'is', 'it', 'ja', 'lt', 'lv', 'mk', 'nl', 'no', 'pl', 'pt-BR', 'pt-PT', 'ro', 'ru', 'sk', 'sv', 'tr', 'ua', 'vi', 'zh-CN', 'zh-TW'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/select2/js/i18n/select2_locale_' ~ app.request.locale ~ '.js') }}"></script>
    {% endif %}

    {% if app.request.locale in ['cs', 'de', 'es', 'eu', 'it', 'nl', 'pl', 'pt-BR', 'ru', 'zh-CN'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-select/js/i18n/defaults-' ~ app.request.locale ~ '.js') }}"></script>
    {% endif %}
{% endblock admingenerator_form_javascripts %}
