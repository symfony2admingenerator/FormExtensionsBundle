{% block s2a_datetime_family_js_prototype_template %}
{% autoescape false %}
{% spaceless %}
    var $widget = $('#{{ id }}_widget');
    var config = {{ config|merge({
        'language': app.request.locale
    })|e4js|raw }};

    $widget.datetimepicker(config);
    {% if widget_value is not empty %}
        $widget.data("DateTimePicker").setDate("{{ widget_value }}");
    {% endif %}

    var copyValue = function() {
        var date = $widget.data("DateTimePicker").getDate();
        $field.val(moment(date).format('{{ widget_format }}'));
    };

    $widget.on("dp.change", copyValue);
    $field.closest('form').on('submit', copyValue);
{% endspaceless %}
{% endautoescape %}
{% endblock s2a_datetime_family_js_prototype_template %}

